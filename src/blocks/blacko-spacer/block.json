{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blackoweb/blackoweb-spacer",
	"version": "0.1.0",
	"title": "Spacer custom",
	"category": "design",
	"icon": "editor-expand",
	"description": "Spacer custom",
	"example": {},
	"attributes": {
		"heightDesktop": {
			"type": "number",
			"default": "120px"
		},
		"heightTablet": {
			"type": "number",
			"default": "60px"
		},
		"heightMobile": {
			"type": "number",
			"default": "30px"
		},
		"width": {
			"type": "number",
			"default": "100%"
		}
	},
	"supports": {
		"anchor": true,
		"alignWide": true,
		"html": false
	},
	"textdomain": "blackoweb-spacer",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}